<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Voltflow.Views.Pages.Cars.AddCarView">
	<StackPanel>
		<TextBox Text="{Binding Name}" Watermark="Name"/>
		<TextBox Text="{Binding BatteryCapacity}" Watermark="Battery capacity"/>
		<TextBox Text="{Binding ChargingRate}" Watermark="Charging rate"/>
		<Button Command="{Binding AddCar}" Content="Add new"/>
		<Button Command="{Binding GoBack}" Content="Go back"/>
	</StackPanel>
</UserControl>
